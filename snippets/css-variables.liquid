{% comment %}
  This is used to create CSS custom variables with Liquid references
  The file variables.scss is referring these CSS custom variables as Sass variables
  to use throughout the stylesheet
{% endcomment %}

{% style %}


:root{

}


@font-face {
    font-family: 'Neutra Text';
    src: url({{ "NeutraText-LightAlt.woff2" | asset_url }}) format('woff2'), url({{ "NeutraText-LightAlt.woff" | asset_url }}) format('woff');
    font-weight: 300;
    font-style: normal;
    font-display: swap;
}
